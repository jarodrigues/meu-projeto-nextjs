generator client {
  provider = "prisma-client-js"
  binaryTargets = ["rhel-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Message {
  id        Int      @id @default(autoincrement())
  name      String   @map("nome")
  email     String?
  message   String   @map("mensagem")
  createdAt DateTime @default(now())
}
