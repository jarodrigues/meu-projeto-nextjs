{
  "buildCommand": "npx prisma generate --schema=./prisma/schema.prisma && next build",
  "outputDirectory": ".next",
  "env": {
    "PRISMA_GENERATE_BINARY_TARGETS": "linux-musl-openssl-3.0.x"
  },
  "functions": {
    "api/**/*.js": {
      "runtime": "nodejs20",
      "maxDuration": 10
    }
  }
}
